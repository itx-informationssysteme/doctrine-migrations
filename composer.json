{
    "name": "itx/doctrine-migrations",
    "description": "TYPO3 extension for versioned migration files, integrating doctrine/migrations",
    "type": "typo3-cms-extension",
	"license": [
        "GPL-3.0-or-later"
    ],
	"support" : {
		"email" : "typo-itx@itx.de",
		"issues" : "https://github.com/itx-informationssysteme/doctrine_migrations/issues",
		"docs" : "https://github.com/itx-informationssysteme/doctrine_migrations"
	},
    "require": {
		"php": ">=8.1",
        "typo3/cms-core": "^11.5",
        "doctrine/migrations": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Itx\\DoctrineMigrations\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "doctrine_migrations"
        }
    }
}
